{
    "name": "@comanda/sdk",
    "version": "1.0.0",
    "description": "Comanda SDK - contracts and HTTP clients for the comanda ecosystem",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./contracts": {
            "import": "./dist/contracts/index.mjs",
            "require": "./dist/contracts/index.js",
            "types": "./dist/contracts/index.d.ts"
        },
        "./clients": {
            "import": "./dist/clients/index.mjs",
            "require": "./dist/clients/index.js",
            "types": "./dist/clients/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "build:mjs": "npm run build && node scripts/generate-mjs.js",
        "dev": "tsc --watch",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "comanda",
        "sdk",
        "http-client",
        "contracts"
    ],
    "author": "Comanda",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.10.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "axios": "^1.13.5"
    },
    "publishConfig": {
        "access": "public"
    }
}
